{
  "name": "back-end",
  "version": "1.0.0",
  "description": "Back-end for Participium project",
  "main": "index.ts",
  "scripts": {
    "start": "ts-node index.ts",
    "dev": "nodemon --exec ts-node index.ts",
    "build": "tsc",
    "test": "cross-env NODE_ENV=test TEST_DB_IN_MEMORY=true jest --runInBand --coverage --colors --detectOpenHandles --forceExit --verbose --testTimeout=60000",
    "test_unit": "cross-env NODE_ENV=test jest --runInBand --coverage --coverageDirectory=coverage/unit --colors --detectOpenHandles --forceExit --verbose --testPathPattern=test_unit --testTimeout=60000",
    "test_integration": "cross-env NODE_ENV=test jest --runInBand --coverage --coverageDirectory=coverage/integration --colors --detectOpenHandles --forceExit --verbose --testPathPattern=test_integration --testTimeout=60000",
    "test_e2e": "cross-env NODE_ENV=test jest --runInBand --coverage --coverageDirectory=coverage/e2e --colors --detectOpenHandles --forceExit --verbose --testPathPattern=test_e2e --testTimeout=60000",
    "test_e2e_fast": "cross-env NODE_ENV=test TEST_DB_IN_MEMORY=true jest --runInBand --colors --verbose --testPathPattern=test_e2e --testTimeout=20000",
    "test_ui": "cross-env NODE_ENV=test jest --runInBand --coverage --coverageDirectory=coverage/ui --colors --detectOpenHandles --forceExit --verbose --testPathPattern=test_ui --testTimeout=90000",
    "reset-database": "node ../database/resetDatabase.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@supabase/supabase-js": "^2.81.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "express-validator": "^7.3.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "selenium-webdriver": "^4.38.0",
    "sqlite3": "^5.1.7",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/chromedriver": "^81.0.5",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jest": "^29.5.3",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.0",
    "@types/selenium-webdriver": "^4.35.4",
    "@types/sqlite3": "^3.1.11",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^10.0.0",
    "chromedriver": "^142.0.3",
    "cross-env": "^7.0.3",
    "jest": "^29.6.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
